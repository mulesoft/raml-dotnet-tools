          application/json:
            type: "Response"
          application/xml:
            type: "Response"
/pdfmarkup/v1/documents/{filePath}.pdf/pages/{pageIndex}/content:
  uriParameters:
    filePath:
      type: "string"
      required: false
    pageIndex:
      type: "string"
      required: false
  get:
    displayName: "Get page content (pdfmarkup)"
    queryParameters:
      print:
        type: "boolean"
        default: false
