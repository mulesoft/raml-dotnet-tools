#%RAML 1.0
title: "Qoppa PDF Processing REST API"
version: "1.0"
protocols:
- "HTTP"
baseUri: "http://www.baseuri.com"
mediaType:
- "*/*"
types:
  WidgetModel:
    type: "object"
    properties:
      rectangle:
        type: "RectangleModel"
      pageIndex:
        type: "integer"
        format: "int32"
  ImageHandlerModel:
    type: "object"
    properties:
      compression:
        type: "integer"
        format: "int32"
      colorSpace:
        type: "integer"
        format: "int32"
      dpi:
        type: "integer"
        format: "int32"
      jpegQuality:
        type: "number"
        format: "float"
  RectangleModel:
    type: "object"
    properties:
      x:
        type: "number"
        format: "double"
      y:
        type: "number"
        format: "double"
      width:
        type: "number"
        format: "double"
      height:
        type: "number"
        format: "double"
  StreamingOutput:
    type: "object"
  LinkModel:
    type: "object"
    properties:
      REF_FILE:
        type: "string"
      REF_FOLDER:
        type: "string"
      REF_ANNOTATIONS:
        type: "string"
      REF_FIELDS:
        type: "string"
      REF_PAGES:
        type: "string"
      REF_PAGE:
        type: "string"
      REF_PASSWORD_PERMISSIONS:
        type: "string"
      REF_ALL_PERMISSIONS:
        type: "string"
      REF_SIGNATURE_FIELDS:
        type: "string"
      REF_CONTENT:
        type: "string"
      REF_TEXT:
        type: "string"
      rel:
        type: "string"
      href:
        type: "string"
  DocumentModel:
    type: "FileModel"
  FileModel:
    type: "object"
    properties:
      path:
        type: "string"
      link:
        type: "LinkModel"
  FieldsModel:
    type: "object"
    properties:
      fields:
        type: "array"
        items:
          type: "FieldModel"
      link:
        type: "LinkModel"
      content:
        type: "LinkModel"
  DigitalIDModel:
    type: "object"
    properties:
      keystorePassword:
        type: "string"
      keyAlias:
        type: "string"
      keyPassword:
        type: "string"
      signReason:
        type: "string"
      signLocation:
        type: "string"
      signContactInformation:
        type: "string"
      certifyingSignature:
        type: "boolean"
      certifyingPermissions:
        type: "integer"
        format: "int32"
      format:
        type: "integer"
        format: "int32"
  PagesModel:
    type: "object"
    properties:
      pageCount:
        type: "integer"
        format: "int32"
      pages:
        type: "array"
        items:
          type: "PageModel"
      link:
        type: "LinkModel"
      content:
        type: "LinkModel"
      text:
        type: "LinkModel"
  PageModel:
    type: "object"
    properties:
      width:
        type: "number"
        format: "double"
      height:
        type: "number"
        format: "double"
      index:
        type: "integer"
        format: "int32"
      annotationCount:
        type: "integer"
        format: "int32"
      link:
        type: "LinkModel"
  FieldModel:
    type: "object"
    properties:
      name:
        type: "string"
      type:
        type: "string"
      userName:
        type: "string"
      widgetCount:
        type: "integer"
        format: "int32"
      widget:
        type: "array"
        items:
          type: "WidgetModel"
  PageActionModel:
    type: "object"
  SignatureFieldModel:
    type: "FieldModel"
  JobStateModel:
    type: "object"
    properties:
      jobStateXML:
        type: "string"
  OptimizeProfileModel:
    type: "object"
    properties:
      profileName:
        type: "string"
      clearSignature:
        type: "boolean"
      discardAltImages:
        type: "boolean"
      discardAnnotations:
        type: "boolean"
      discardBookmarks:
        type: "boolean"
      discardDocumentInfo:
        type: "boolean"
      discardFileAttachments:
        type: "boolean"
      discardFormFields:
        type: "boolean"
      discardJSActions:
        type: "boolean"
      discardLinks:
        type: "boolean"
      discardPageThumbnails:
        type: "boolean"
      discardUnusedResources:
        type: "boolean"
      discardXMPMetadata:
        type: "boolean"
      flateUncompressedStreams:
        type: "boolean"
      flattenAnnotations:
        type: "boolean"
      flattenFormFields:
        type: "boolean"
      linearize:
        type: "boolean"
      mergeDuplicateFonts:
        type: "boolean"
      mergeDuplicateImages:
        type: "boolean"
      compressObjectsIntoStreams:
        type: "boolean"
      colorImageHandler:
        type: "ImageHandlerModel"
      grayImageHandler:
        type: "ImageHandlerModel"
      bwImageHandler:
        type: "ImageHandlerModel"
  void:
    type: "object"
  ChoiceModel:
    type: "object"
    properties:
      position:
        type: "integer"
        format: "int32"
      save:
        type: "string"
      text:
        type: "string"
  AnnotationsModel:
    type: "object"
    properties:
      annotations:
        type: "array"
        items:
          type: "AnnotationModel"
      link:
        type: "LinkModel"
      content:
        type: "LinkModel"
  Response:
    type: "object"
  OptimizeProfilesModel:
    type: "object"
    properties:
      optimizeProfiles:
        type: "array"
        items:
          type: "OptimizeProfileModel"
  ChoiceListModel:
    type: "object"
    properties:
      choiceList:
        type: "array"
        items:
          type: "ChoiceModel"
  SignatureFieldsModel:
    type: "object"
    properties:
      signatureFields:
        type: "array"
        items:
          type: "SignatureFieldModel"
  PreflightResultRecordModel:
    type: "object"
    properties:
      rectangle:
        type: "RectangleModel"
      detail:
        type: "string"
      header:
        type: "string"
      pageNumber:
        type: "integer"
        format: "int32"
  DocumentOverviewModel:
    type: "object"
    properties:
      author:
        type: "string"
      creationDate:
        type: "date-only"
      creator:
        type: "string"
      modDate:
        type: "date-only"
      pageCount:
        type: "integer"
        format: "int32"
      producer:
        type: "string"
      subject:
        type: "string"
      title:
        type: "string"
  AnnotationModel:
    type: "object"
    properties:
      contents:
        type: "string"
      creator:
        type: "string"
      modifiedDate:
        type: "date-only"
      subject:
        type: "string"
      rectangle:
        type: "RectangleModel"
  PreflightInformationModel:
    type: "object"
    properties:
      profile:
        type: "string"
      pasVersion:
        type: "string"
      date:
        type: "date-only"
      duration:
        type: "integer"
        format: "int64"
  PDFPermissionsModel:
    type: "object"
    properties:
      passwordPermissions:
        type: "PermissionsModel"
      docMDPPermissions:
        type: "PermissionsModel"
      usageRightsPermissions:
        type: "PermissionsModel"
      signaturePermissions:
        type: "PermissionsModel"
  PreflightResultsModel:
    type: "object"
    properties:
      resultRecords:
        type: "array"
        items:
          type: "PreflightResultRecordModel"
      preflightInfo:
        type: "PreflightInformationModel"
      documentInfo:
        type: "DocumentOverviewModel"
  JobStatesModel:
    type: "object"
    properties:
      jobStates:
        type: "array"
        items:
          type: "JobStateModel"
  PermissionsModel:
    type: "object"
    properties:
      assembleDocumentAllowed:
        type: "boolean"
      changeDocumentAllowed:
        type: "boolean"
      extractTextAllowed:
        type: "boolean"
      extractTextForAccessibilityAllowed:
        type: "boolean"
      fillFormFieldsAllowed:
        type: "boolean"
      modifyAnnotationsAllowed:
        type: "boolean"
      printAllowed:
        type: "boolean"
      printHighResolutionAllowed:
        type: "boolean"
  FolderModel:
    type: "FileModel"
/pdfmarkup/v1/documents/{filePath}.pdf:
  uriParameters:
    filePath:
      type: "string"
      required: false
  get:
    displayName: "Get document (pdfmarkup)"
    responses:
      200:
        body:
          application/json:
            type: "DocumentModel"
          application/xml:
            type: "DocumentModel"
/pdfmarkup/v1/documents/{filePath}.pdf/pages/{pageIndex}/prepareForPrint:
  uriParameters:
    filePath:
      type: "string"
      required: false
    pageIndex:
      type: "string"
      required: false
  post:
    displayName: "Prepare page for print (pdfmarkup)"
    body:
      application/vnd.adobe.xfdf:
        type: "file"
    responses:
      200:
        body:
          application/json:
            type: "Response"
          application/xml:
            type: "Response"
/pdfmarkup/v1/documents/{filePath}.pdf/annotations/content:
  uriParameters:
    filePath:
      type: "string"
      required: false
  get:
    displayName: "Get annotation content (pdfmarkup)"
    responses:
      200:
        body:
          application/vnd.adobe.xfdf:
            type: "Response"
/pdfmarkup/v1/documents/{filePath}.pdf/annotations/delete:
  uriParameters:
    filePath:
      type: "string"
      required: false
  post:
    displayName: "Delete annotations (pdfmarkup)"
    body:
      application/vnd.adobe.xfdf:
        type: "file"
    responses:
      200:
        body:
          application/json:
            type: "AnnotationsModel"
          application/xml:
