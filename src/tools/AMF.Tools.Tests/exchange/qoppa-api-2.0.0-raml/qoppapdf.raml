            type: "Response"
/qoppapdf/v1/documents/{filePath}.pdf:
  uriParameters:
    filePath:
      type: "string"
      required: false
  post:
    displayName: "Perform a document action"
    responses:
      200:
        body:
          application/json:
            type: "Response"
          application/xml:
            type: "Response"
  get:
    displayName: "Get document"
    responses:
      200:
        body:
          application/json:
            type: "DocumentModel"
          application/xml:
            type: "DocumentModel"
  delete:
    displayName: "Delete document"
    responses:
      200:
        body:
          application/json:
            type: "Response"
          application/xml:
            type: "Response"
  put:
    displayName: "Upload document"
    queryParameters:
      dpi:
        type: "integer"
        format: "int32"
        default: 72
        required: false
    headers:
      Content-Type:
        type: "string"
        default: "application/pdf"
        required: false
    body:
      application/pdf:
        type: "file"
      image/gif:
        type: "file"
      image/jpeg:
        type: "file"
      image/png:
        type: "file"
      image/tiff:
        type: "file"
      text/plain:
        type: "file"
      application/msword:
        type: "file"
      application/vnd.openxmlformats-officedocument.wordprocessingml.document:
        type: "file"
      application/vnd.openxmlformats-officedocument.spreadsheetml.sheet:
        type: "file"
    responses:
      200:
        body:
          application/json:
            type: "Response"
          application/xml:
            type: "Response"
/qoppapdf/v1/documents/{filePath}.pdf/content:
  uriParameters:
